#logs.tab-pane
    .panel.panel-default
        .panel-heading
            h2.panel-title 取得した位置情報
        .panel-body
            table.table
                thead
                    tr
                        th No.
                        th 位置情報
                        th 取得日時
                tbody
                    tr(v-for="x in logs")
                        td {{ $index }}
                        td {{ x.lat }},{{ x.lon }}
                        td {{ x.createdAt }}